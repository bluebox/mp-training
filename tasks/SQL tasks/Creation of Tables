

"""Creating and Using Database"""


create database Employees;

use Employees;


"""Creating Tables"""


create table Employee_Table(emp_id varchar(20), name varchar(20), immediate_head_id varchar(20), dept_id varchar(20), insurance_id
varchar(20), employee_info_id varchar(20));


create table Department_Table(dept_id varchar(20),dept_name varchar(20));



create table Insurance_Table(insurance_id varchar(20),insurance_name varchar(20));



create table Office_Table (office_id varchar(20), office_name varchar(20), place varchar(20));


create table Employee_Office_Table(id varchar(20), emp_id varchar(20), office_id varchar(20));


create table Employee_Info_Table(employee_info_id varchar(20), DOJ varchar(20), salary varchar(20));


"""Adding Key Constraints to Tables"""


alter table Employee_Table add Primary key(emp_id);
alter table Department_Table add Primary Key(dept_id);
alter table Insurance_Table add Primary Key(insurance_id);
alter table Office_Table add primary key(office_id);
alter table Employee_Office_Table add primary key(id);
alter table Employee_Info_Table add primary key(employee_info_id);



alter table Employee_Table add foreign key (dept_id) references Department_Table(dept_id);
alter table Employee_Table add foreign key (insurance_id) references Insurance_Table(insurance_id);
alter table Employee_Table add foreign key (employee_info_id) references Employee_Info_Table(employee_info_id);

alter table Employee_Office_Table add foreign key (emp_id) references Employee_Table(emp_id)
alter table Employee_Office_Table add foreign key(office_id) references Office_Table(office_id)


