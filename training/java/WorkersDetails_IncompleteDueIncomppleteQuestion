public class Worker{
    private String name;
    private String birthDate;
    private String endDate;
    public Worker(String name,birthDate,endDate){
        this.name=name;
        this.birthDate=birthDate;
        this.endDate=endDate;
    
    }
    public static int getAge(String presentYear){
        return (int)presentYear-(int)this.birthDate;
    }
    public static double collectPay(){

    }
    public static void terminate(String endDate){
        this.endDate=endDate;
    }
}