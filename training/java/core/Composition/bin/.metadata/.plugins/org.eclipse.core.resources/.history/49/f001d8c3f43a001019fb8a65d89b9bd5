package differentoperators;

public class ArithematicOperators {
	public static void main(String[] args)
	{
		int a=5;
		int b=10;
		int sum=a+b;
		int sub = a-b;
		int mul=a*b;
		int div=a/b;
		System.out.println(sum);
		System.out.println(sub);
		System.out.println(mul);
		System.out.println(div);
		
	}
	
}
