package ExpressionsStatementsMethodOverloading;
import java.util.*;

public class Switch {
	Scanner sc = new Scanner(System.in);
	int  n =sc.nextInt();
	String ans="";
	switch(n) {
	case 0: 
		ans="Sunday";
		break;
	case 1: 
		ans="Sunday";
		break;
	case 2: 
		ans="Sunday";
		break;
	case 3: 
		ans="Sunday";
		break;
	case 4: 
		ans="Sunday";
		break;
	case 5: 
		ans="Sunday";
		break;
	case 6: 
		ans="Sunday";
		break;
	
	}
	System.out.println(ans);
	

}
