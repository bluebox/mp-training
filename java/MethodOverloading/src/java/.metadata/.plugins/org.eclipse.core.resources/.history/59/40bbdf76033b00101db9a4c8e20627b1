package loops;

public class Listfactors {
	public static void main(String [] args) {
		int number= 12;
		int counter = 1;
		for(counter=1; counter<=number; counter++) {
			printFactor(counter,number);
		}
	}
	public static void printFactor(int counter, int number) {
		if(number % counter == 0) {
			System.out.println(counter);
		}
	}

}
